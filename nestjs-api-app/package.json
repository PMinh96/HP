{
  "name": "nestjs-api-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dker:s:t": "docker start test-database",
    "dker:s:d": "docker start dev-database",
    "dker:s:a": " yarn dker:s:d && dker:s:t",
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:remove": "docker compose rm dev-database --stop --force --volumes",
    "db:dev:create": "docker compose up dev-database --detach",
    "db:dev:restart": "yarn db:dev:remove && yarn db:dev:create && timeout /t 2 && yarn prisma:dev:deploy",
    "base-d-run": "npx dotenv -e .env -- prisma studio",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:remove": "docker compose rm test-database --stop --force --volumes",
    "db:test:create": "docker compose up test-database --detach",
    "db:test:restart": "yarn db:test:remove && yarn db:test:create && timeout /t 2 && yarn prisma:test:deploy",
    "pretest:e2e": "yarn db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache --config ./test/jest-e2e.json",
    "base-t-run": "npx dotenv -e .env.test -- prisma studio",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "argon2": "^0.40.1",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "google-auth-library": "^9.9.0",
    "googleapis": "^135.0.0",
    "mssql": "^10.0.4",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.3",
    "nestjs-paginate": "^8.6.3",
    "nestjs-rate-limiter": "^3.1.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "rate-limiter-flexible": "^5.0.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "pactum": "^3.6.6",
    "prettier": "^3.0.0",
    "prisma": "^5.11.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3",
    "webpack": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
